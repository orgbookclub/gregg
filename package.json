{
  "name": "gregg",
  "author": "ravsodhi",
  "version": "0.1.3",
  "description": "Discord Bot",
  "main": "./dist/index.js",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "discordjs",
    "discord",
    "bot",
    "discord-bot",
    "organized-book-club"
  ],
  "repository": "https://github.com/orgbookclub/gregg",
  "bugs": {
    "url": "https://github.com/orgbookclub/gregg/issues"
  },
  "homepage": "https://gregg-bot.azurewebsites.net/",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postinstall": "prisma generate",
    "lint": "eslint src --max-warnings 0 && prettier src --check",
    "lint:fix": "eslint src --max-warnings 0 --fix && prettier src test --write",
    "start": "node ./dist/index.js",
    "start:dev": "tsc && node -r dotenv/config ./dist/index.js",
    "docs:serve": "docsify serve docs"
  },
  "dependencies": {
    "@orgbookclub/ows-client": "^0.3.0",
    "@prisma/client": "^5.0.0",
    "@sentry/node": "^7.48.0",
    "cron": "^2.3.1",
    "discord.js": "^14.11.0",
    "express": "^4.18.2",
    "glob": "^10.3.3",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.1",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.60.0",
    "docsify-cli": "^4.4.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prisma": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18.14.2",
    "yarn": "1.22.19"
  }
}
